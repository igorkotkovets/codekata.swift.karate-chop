# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'Common_iOS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Common_iOS

  target 'Common_iOSTests' do
    inherit! :search_paths
    pod 'CleanTests', :git => 'https://github.com/igorkotkovets/pod.swift.clean-tests.git', :branch => 'master'
  end

end

target 'Common_macOS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Common_macOS

  target 'Common_macOSTests' do
    inherit! :search_paths
    # Pods for testing
  end

end

target 'KarateChop' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for KarateChop

  target 'KarateChopTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
